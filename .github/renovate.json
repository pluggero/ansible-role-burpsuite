{
  "extends": [
    "config:best-practices",
    ":disableRateLimiting"
  ],
  "automerge": false,
  "automergeType": "pr",
  "packageRules": [
    {
      "matchManagers": ["github-actions", "pip_requirements"],
      "automerge": true,
      "automergeType": "pr"
    },
    {
      "matchManagers": ["custom.regex"],
      "matchDepNames": ["python/cpython"],
      "automerge": true,
      "automergeType": "pr"
    },
    {
      "matchManagers": ["custom.regex"],
      "automerge": false
    }
  ],
  "enabledManagers": [
    "github-actions",
    "pip_requirements",
    "custom.regex"
  ],
  "assignees": ["pluggero"],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^defaults/main\\.yml$/"
      ],
      "matchStrings": [
        "burpsuite_version:\\s+\"(?<currentValue>[\\d\\.]+)\""
      ],
      "datasourceTemplate": "custom.burpsuite",
      "depNameTemplate": "burpsuite",
      "versioningTemplate": "loose"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^defaults/main\\.yml$/"
      ],
      "matchStrings": [
        "burpsuite_extension_runtime_jython_version:\\s+\"(?<currentValue>[\\d\\.]+)\""
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "jython/jython",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^defaults/main\\.yml$/"
      ],
      "matchStrings": [
        "burpsuite_extension_runtime_jruby_version:\\s+\"(?<currentValue>[\\d\\.]+)\""
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "jruby/jruby",
      "versioningTemplate": "ruby"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "\\.github/workflows/.+\\.ya?ml$"
      ],
      "matchStrings": [
        "python-version:\\s+[\"'](?<currentValue>\\d+\\.\\d+)[\"']"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "python/cpython",
      "versioningTemplate": "semver"
    }
  ],
  "customDatasources": {
    "burpsuite": {
      "defaultRegistryUrlTemplate": "https://portswigger.net/burp/releases/data?lastId=-1&pageSize=5",
      "format": "json",
      "transformTemplates": [
        "{ \"releases\": $distinct(ResultSet.Results[releaseChannels[$contains(\"Stable\")]].builds[ProductEdition = \"Burp Suite Community Edition\"].{\"version\": Version}) }"
      ]
    }
  }
}
